{
    "version": 2,
    "rewrites": [{ "source": "/(.*)", "destination": "/" }] ,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
